# ComfyUI Custom API 标准配置
# 说明见 config.yaml.example

# 全局 API 密钥定义（可选；也可只用环境变量）
env_api_key:
  - name: VECTORENGINE_GEMINI_API_KEY
    # value: "your-api-key-here"   # 可选，不填则从环境变量读取
    description: "Gemini 图像模型 API 密钥"
  - name: GEMINI_API_KEY
    description: "Google Gemini API 密钥"
  - name: VECTORENGINE_API_KEY
    description: "VectorEngine 平台 API 密钥"

# 平台列表：不同 provider 对应不同 base_url 与 type
providers:
  - name: vectorengine
    type: openai_v1
    api_key_env: VECTORENGINE_API_KEY
    base_url: https://api.vectorengine.ai
    models:
      gemini-3-pro-image-preview:
        model_name: gemini-3-pro-image
        api_key_env: VECTORENGINE_GEMINI_API_KEY
        type: google

  - name: google_gemini
    type: google
    api_key_env: GEMINI_API_KEY
    models:
      gemini-3-pro-image-preview:
        model_name: gemini-3-pro-image
      flash:
        model_name: gemini-2.5-flash-preview-05-20
      pro:
        model_name: gemini-2.5-pro-preview-06-05

# 节点与可用模型：每个节点只展示此处配置的 provider/model
node_config:
  - name: GeminiImageProNode
    models:
      - provider: vectorengine
        model: gemini-3-pro-image-preview
        is_default: true
      - provider: google_gemini
        model: gemini-3-pro-image-preview

  - name: GeminiImageNode
    models:
      - provider: vectorengine
        model: gemini-3-pro-image-preview
        is_default: true
      - provider: google_gemini
        model: gemini-3-pro-image-preview

  - name: GeminiNode
    models:
      - provider: google_gemini
        model: flash
        is_default: true
      - provider: google_gemini
        model: pro

  - name: GeminiInputFilesNode
    models: []
